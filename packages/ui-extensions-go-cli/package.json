{
  "name": "@shopify/ui-extensions-go-cli",
  "version": "3.7.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm clean && pnpm --dir ../.. exec nx run ui-extensions-go-cli:build",
    "build:nx": "node bin/build.js",
    "clean": "rimraf api/dev-console/",
    "test": "pnpm clean && pnpm --dir ../.. exec nx run ui-extensions-go-cli:test",
    "test:nx": "node bin/test.js",
    "package": "pnpm --dir ../.. exec nx run ui-extensions-go-cli:package",
    "package:nx": "node bin/package.js",
    "lint": "gofmt -s .",
    "lint:fix": "gofmt -w -s ."
  },
  "devDependencies": {
    "@shopify/shopify-cli-extensions": "workspace:*"
  }
}
