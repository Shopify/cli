{
  "name": "@shopify/cli-kit",
  "version": "3.5.0",
  "private": false,
  "description": "A set of utilities, interfaces, and models that are common across all the platform features",
  "keywords": [
    "shopify",
    "shopify-cli",
    "shopify-partners"
  ],
  "license": "MIT",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./node/*": {
      "node": "./dist/node/*.js",
      "types": "./dist/node/*.d.ts"
    },
    "./common/*": {
      "node": "./dist/common/*.js",
      "types": "./dist/common/*.d.ts"
    },
    "./testing/*": {
      "node": "./dist/testing/*.js",
      "types": "./dist/testing/*.d.ts"
    }
  },
  "files": [
    "/dist",
    "/assets"
  ],
  "publishConfig": {
    "access": "public",
    "@shopify:registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "clean": "shx rm -rf dist",
    "build": "rimraf dist/ && tsc -b ./tsconfig.build.json",
    "prepack": "cross-env NODE_ENV=production yarn run build",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "tsc": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ]
  },
  "engine-strict": true,
  "engines": {
    "node": ">=14.13.1"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "dependencies": {
    "@bugsnag/js": "^7.16.7",
    "@iarna/toml": "^2.2.5",
    "@oclif/core": "^1.0",
    "@types/archiver": "^5.3.1",
    "abort-controller": "^3.0.0",
    "ansi-colors": "^4.1.1",
    "archiver": "^5.3.1",
    "change-case": "^4.1.2",
    "color-json": "^2.0.1",
    "commondir": "^1.0.1",
    "conf": "^10.1.2",
    "cross-zip": "^4.0.0",
    "del": "^6.0.0",
    "enquirer": "^2.3.6",
    "env-paths": "^3.0.0",
    "envfile": "^6.17.0",
    "execa": "^6.0.0",
    "fast-glob": "^3.2.11",
    "fastify": "^4.2.1",
    "find-process": "^1.4.7",
    "find-up": "^6.2.0",
    "find-versions": "^5.0.0",
    "form-data": "^4.0.0",
    "fs-extra": "^10.0.0",
    "get-port-please": "^2.5.0",
    "graphql": "^16.4.0",
    "graphql-request": "^4.3.0",
    "inquirer": "^8.2.4",
    "is-interactive": "^2.0.0",
    "js-yaml": "^4.1.0",
    "keytar": "^7.9.0",
    "kill-port-process": "^3.1.0",
    "latest-version": "^6.0.0",
    "liquidjs": "^9.36.0",
    "listr2": "^4.0.5",
    "log-update": "^4.0.0",
    "md5-file": "^5.0.0",
    "node-fetch": "^3.2.4",
    "open": "^8.4.0",
    "pathe": "0.2.0",
    "prettier": "^2.6.2",
    "semver": "^7.3.6",
    "simple-git": "^3.5.0",
    "source-map-support": "^0.5.21",
    "stacktracey": "^2.1.8",
    "strip-ansi": "^7.0.1",
    "tempy": "^3.0.0",
    "term-size": "^3.0.1",
    "terminal-link": "^3.0.0",
    "ts-error": "^1.0.6",
    "unique-string": "^3.0.0",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@types/commondir": "^1.0.0",
    "@types/cross-zip": "^4.0.0",
    "@types/inquirer": "^8.2.1",
    "@types/js-yaml": "^4.0.5",
    "@types/semver": "^7.3.9",
    "node-stream-zip": "^1.15.0",
    "vitest": "^0.17.1"
  }
}
