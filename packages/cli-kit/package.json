{
  "name": "@shopify/cli-kit",
  "version": "3.38.0",
  "private": false,
  "description": "A set of utilities, interfaces, and models that are common across all the platform features",
  "keywords": [
    "shopify",
    "shopify-cli",
    "shopify-partners"
  ],
  "bugs": {
    "url": "https://github.com/Shopify/cli/issues/new/choose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Shopify/cli/edit/main/packages/cli-kit"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "node": "./dist/public/*.js",
      "types": "./dist/public/*.d.ts"
    },
    "./testing/*": {
      "node": "./dist/testing/*.js",
      "types": "./dist/testing/*.d.ts"
    },
    "./typing/*": {
      "node": "./dist/typing/*.js",
      "types": "./dist/typing/*.d.ts"
    }
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist",
    "/assets"
  ],
  "scripts": {
    "build": "nx build",
    "clean": "nx clean",
    "lint": "nx lint",
    "lint:fix": "nx lint:fix",
    "prepack": "cross-env NODE_ENV=production pnpm nx build && cp ../../README.md README.md",
    "test": "nx run cli-kit:test",
    "test:coverage": "nx test:coverage",
    "test:watch": "nx test:watch",
    "type-check": "nx type-check"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ],
    "overrides": [
      {
        "files": [
          "**/node/cli.ts"
        ],
        "rules": {
          "rulesdir/specific-imports-in-bootstrap-code": [
            "error",
            {
              "dynamic": [
                "./node-package-manager.js",
                "./error-handler.js",
                "../../public/node/environment/local.js",
                "./environment/local.js",
                "@oclif/core",
                "../../private/node/environment/utilities.js",
                "../../private/node/constants.js",
                "./path.js",
                "./system.js"
              ],
              "static": [
                "@oclif/core"
              ]
            }
          ]
        }
      }
    ]
  },
  "dependencies": {
    "@bugsnag/js": "7.16.7",
    "@iarna/toml": "2.2.5",
    "@oclif/core": "1.21.0",
    "@types/archiver": "5.3.1",
    "abort-controller": "3.0.0",
    "ansi-escapes": "6.0.0",
    "archiver": "5.3.1",
    "chalk": "5.1.0",
    "change-case": "4.1.2",
    "color-json": "3.0.0",
    "commondir": "1.0.1",
    "conf": "11.0.1",
    "cross-zip": "4.0.0",
    "deepmerge": "4.2.2",
    "del": "6.0.0",
    "env-paths": "3.0.0",
    "envfile": "6.18.0",
    "execa": "6.0.0",
    "fast-glob": "3.2.11",
    "figures": "5.0.0",
    "find-process": "1.4.7",
    "find-up": "6.2.0",
    "find-versions": "5.0.0",
    "form-data": "4.0.0",
    "fs-extra": "11.1.0",
    "fuzzy": "0.1.3",
    "get-port-please": "3.0.1",
    "gradient-string": "2.0.2",
    "graphql": "16.4.0",
    "graphql-request": "4.3.0",
    "h3": "0.7.21",
    "ink": "3.2.0",
    "is-interactive": "2.0.0",
    "js-yaml": "4.1.0",
    "keytar": "7.9.0",
    "kill-port-process": "3.1.0",
    "latest-version": "7.0.0",
    "liquidjs": "9.36.0",
    "listr2": "4.0.5",
    "lodash": "4.17.21",
    "log-update": "4.0.0",
    "macaddress": "0.5.3",
    "node-abort-controller": "3.0.1",
    "node-fetch": "3.2.4",
    "open": "8.4.0",
    "pathe": "1.0.0",
    "prettier": "2.7.1",
    "react": "17.0.2",
    "semver": "7.3.6",
    "simple-git": "3.16.0",
    "source-map-support": "0.5.21",
    "stacktracey": "2.1.8",
    "strip-ansi": "7.0.1",
    "tempy": "3.0.0",
    "term-size": "3.0.1",
    "terminal-link": "3.0.0",
    "tree-kill": "1.2.2",
    "ts-error": "1.0.6",
    "unique-string": "3.0.0",
    "zod": "3.17.3"
  },
  "devDependencies": {
    "@types/commondir": "^1.0.0",
    "@types/cross-zip": "^4.0.0",
    "@types/diff": "^5.0.2",
    "@types/fs-extra": "9.0.13",
    "@types/gradient-string": "^1.1.2",
    "@types/lodash": "4.14.191",
    "@types/node": "14.17.0",
    "@types/react": "17.0.2",
    "@types/semver": "^7.3.9",
    "@vitest/coverage-istanbul": "^0.26.3",
    "ink-testing-library": "^2.1.0",
    "node-stream-zip": "^1.15.0",
    "vite": "^2.9.13",
    "vitest": "^0.26.3"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "publishConfig": {
    "@shopify:registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "engine-strict": true
}
