{
  "name": "features",
  "root": "packages/features",
  "sourceRoot": "packages/features",
  "projectType": "library",
  "implicitDependencies": ["create-app", "create-hydrogen", "cli-main"],
  "tags": ["scope:e2e"],
  "targets": {
    "test": {
      "executor": "nx:run-commands",
      "dependsOn": ["^build"],
      "options": {
        "command": "node_modules/.bin/cucumber-js -p default -c cucumber.js",
        "cwd": "packages/features"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn run eslint \"**/*.ts\"",
        "cwd": "packages/features"
      }
    },
    "lint:fix": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn run eslint '**/*.ts' --fix",
        "cwd": "packages/features"
      }
    },
    "tsc": {
      "executor": "nx:run-commands",
      "options": {
        "command": "../../node_modules/.bin/tsc --noEmit",
        "cwd": "packages/features"
      }
    }
  }
}
