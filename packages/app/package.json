{
  "name": "@shopify/app",
  "version": "0.33.1",
  "description": "Utilities for loading, building, and publishing apps.",
  "type": "module",
  "files": [
    "/dist",
    "/templates"
  ],
  "publishConfig": {
    "access": "public",
    "@shopify:registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "clean": "shx rm -rf dist",
    "build": "rimraf dist/ && rollup -c && tsc -p tsconfig.dist.json",
    "prepack": "cross-env NODE_ENV=production yarn run build",
    "lint": "prettier -c src/** && eslint 'src/**/*.ts'",
    "lint:fix": "prettier -w src/** && eslint 'src/**/*.ts' --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "tsc": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ]
  },
  "dependencies": {
  },
  "peerDependencies": {
    "@oclif/core": "1.3.6",
    "@shopify/cli-kit": "0.33.1",
    "react": ">=17.0.0 <18.0.0"
  },
  "engine-strict": true,
  "engines": {
    "node": "^14.13.1 || ^16.0.0 || ^17.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "oclif": {
    "commands": "dist/commands"
  },
  "devDependencies": {
    "react-router": "^6.2.2",
    "react-router-dom": "^6.2.2",
    "vite-plugin-pages": "^0.21.2",
    "vitest": "0.7.9"
  }
}
