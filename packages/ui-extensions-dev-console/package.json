{
  "private": true,
  "name": "@shopify/ui-extensions-dev-console-app",
  "version": "3.8.0",
  "scripts": {
    "build": "yarn clean && yarn --cwd ../.. nx run ui-extensions-dev-console:build",
    "build:nx": "tsc --project tsconfig.build.json --noEmit && vite build",
    "clean": "rimraf dist/ ../ui-extensions-go-cli/api/dev-console",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "serve": "vite preview",
    "start": "vite",
    "test": "vitest"
  },
  "dependencies": {
    "@shopify/polaris": "^6.6.0",
    "@shopify/polaris-icons": "^4.6.2",
    "@shopify/react-i18n": "^6.1.0",
    "@shopify/ui-extensions-server-kit": "3.8.0",
    "@vitejs/plugin-react": "^2.0.1",
    "copy-to-clipboard": "^3.3.1",
    "qrcode.react": "^1.0.1",
    "react": "16.14.0",
    "react-dom": "^16.9.11"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": "off"
    }
  },
  "devDependencies": {
    "@shopify/react-testing": "^3.0.0",
    "@shopify/ui-extensions-test-utils": "3.8.0",
    "@types/qrcode.react": "^1.0.2",
    "@types/react": "16.14.0",
    "@types/react-dom": "^16.9.11",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "sass": "^1.42.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.6.4",
    "vite": "^2.4.4",
    "vite-tsconfig-paths": "^3.3.14",
    "vitest": "^0.22.1"
  }
}
