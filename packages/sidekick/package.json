{
  "name": "@shopify/sidekick",
  "version": "3.90.0",
  "packageManager": "pnpm@10.11.1",
  "private": true,
  "description": "Sidekick AI assistant for Shopify CLI",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["/dist", "/oclif.manifest.json"],
  "scripts": {
    "build": "nx build",
    "clean": "nx clean",
    "lint": "nx lint",
    "lint:fix": "nx lint:fix",
    "prepack": "NODE_ENV=production pnpm nx build && cp ../../README.md README.md",
    "vitest": "vitest",
    "type-check": "nx type-check"
  },
  "eslintConfig": {
    "extends": ["../../.eslintrc.cjs"]
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.22.0",
    "@oclif/core": "4.5.3",
    "@shopify/cli-kit": "3.90.0"
  },
  "devDependencies": {
    "@vitest/coverage-istanbul": "^3.1.4"
  },
  "engines": {
    "node": ">=20.10.0"
  },
  "os": ["darwin", "linux", "win32"],
  "publishConfig": {
    "@shopify:registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "engine-strict": true
}
